{
    "initMode": "new",
    "pgParameters": {
        "log_destination": "stderr",
        "logging_collector": "on",
        "log_directory": "log",
        "log_filename": "postgresql-%a.log",
        "log_truncate_on_rotation": "on",
        "log_rotation_age": "1d",
        "log_rotation_size": "0",
        "log_connections": "on",
        "log_disconnections": "on",
        "log_line_prefix": "%m [%p] ",
        "log_timezone": "PRC"
    }
}